{
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "functions": {
    "manage.py": {
      "runtime": "python3.12",
      "handler": "LostNFound.wsgi.application"
    }
  }
}
