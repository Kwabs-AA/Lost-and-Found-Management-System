{
  "builds": [
    {
      "src": "LostNFound/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.10",
        "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "LostNFound/wsgi.py"
    }
  ]
}